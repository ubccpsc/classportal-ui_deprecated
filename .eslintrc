{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/require-default-props": 0,
    "import/no-extraneous-dependencies": [
      0,
      {
        "devDependencies": true
      }
    ],
    "arrow-parens": 0,
    "arrow-body-style": 0,
    "no-plusplus": 0,
    "no-console": 0,
    "no-alert": 0,
    "comma-dangle": 0,
    "object-curly-spacing": 0
  }
}
